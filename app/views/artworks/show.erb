<div class='prime'>
  <% if @home %>
    <a href="/artworks/<%= @artwork.id %>/edit" >[Edit]</a>
    <a href="/artworks/<%= @artwork.id %>/delete" >[Delete]</a>
  <% end %>
</div>
<section class='artwork'>
  <a href="/artworks/<%= @artwork.id %>"><img src="<%= @artwork.image_url %>"></a>
  <p><%= @artwork.title %></p>
  <p><%= @artwork.date %></p>
  <% if !@home %>
    <a href="/lovers/<%= @artwork.artist.id %>"><%= @artwork.artist.alias %></a>
  <% end %>
  <p><%= @artwork.description %></p>
  <%= erb :'artworks/_like', locals: {home: @home, artwork: @artwork} %>
</section>